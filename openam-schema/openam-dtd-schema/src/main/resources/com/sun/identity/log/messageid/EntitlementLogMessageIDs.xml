    "jar://com/sun/identity/log/messageid/amLogMessage.dtd"
>

<logmessages prefix="ENTITLEMENT">

    <logmessage name="ATTEMPT_ADD_PRIVILEGE" id="1"
	loglevel="LL_INFO"
	description="Attempt to add privilege.">
	<datainfo>
	    <item>realm</item>
	    <item>privilege name</item>
	</datainfo>
	<triggers>
	    <item>Add privilege API is called.</item>
	</triggers>
	<actions />
    </logmessage>
    <logmessage name="SUCCEEDED_ADD_PRIVILEGE" id="2"
	loglevel="LL_INFO"
	description="Privilege is added.">
	<datainfo>
	    <item>realm</item>
	    <item>privilege name</item>
	</datainfo>
	<triggers>
	    <item>Add privilege API is called.</item>
	</triggers>
        <actions />
    </logmessage>
    <logmessage name="FAILED_ADD_PRIVILEGE" id="3"
	loglevel="LL_INFO"
	description="Failed to add privilege.">
	<datainfo>
	    <item>realm</item>
	    <item>privilege name</item>
	    <item>error message</item>
	</datainfo>
	<triggers>
	    <item>Add privilege API is called.</item>
	</triggers>
	<actions>
            <item>Privilege might already exists.</item>
            <item>Administrator might not have the permission to add privilege.</item>
	</actions>
    </logmessage>

